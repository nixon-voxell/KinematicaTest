.breadcrumb .breadcrumbItem #breadcrumbLabel {
    color: #B4B4B4;
}

.breadcrumb .breadcrumbItem {
    background-image: resource("Builtin Skins/LightSkin/Images/breadcrump mid");
    background-color: #CBCBCB;
}

.breadcrumb .breadcrumbItem.breadcrumbButton:active {
    background-image: resource("Builtin Skins/LightSkin/Images/breadcrump mid act");
    background-color: #CBCBCB;
}

.breadcrumb .breadcrumbItem.breadcrumbButton.first:active {
    background-image: resource("Builtin Skins/LightSkin/Images/breadcrump left act");
    background-color: #CBCBCB;
}

.breadcrumb .breadcrumbItem.first {
    background-image: resource("Builtin Skins/LightSkin/Images/breadcrump left.png");
    background-color: #CBCBCB;
}
.breadcrumb .breadcrumbItem #breadcrumbLabel {
    color: #000000;
}