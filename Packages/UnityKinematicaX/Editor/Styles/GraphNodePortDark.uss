GraphNodePort {
    --port-color: rgb(200, 200, 00);
    --disabled-port-color: rgb(0, 70, 70);
}

GraphNodePort > #connector {
    border-color: rgb(255, 255, 0);
    background-color: #212121;
}

GraphNodePort > #connector:hover {
    border-color:  #f0f0f0
}

GraphNodePort > #connector > #cap
{
    background-color: #212121;
}

GraphNodePort > #connector > #cap:hover
{
    background-color: #f0f0f0;
}

.Port_Task {
    --port-color: #2080FF;
}